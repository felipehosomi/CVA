  <data name="CarrierVehicle_Delete" xml:space="preserve">
    <value>DELETE FROM [@CVA_CAR_VEHICLE]
WHERE U_CarrierCode = '{0}'</value>
  </data>
  <data name="CarrierVehicle_Get" xml:space="preserve">
    <value>SELECT 
	VEH.Code		[Veículo],
	TYP.Name		[Tipo],
	VEH.U_Plate		[Placa],
	VEH.U_Color		[Cor],
	VEH.U_Driver	[Motorista]
 FROM [@CVA_CAR_VEHICLE] CAR
	INNER JOIN [@CVA_VEHICLE] VEH
		ON CAR.U_CodeVehicle = VEH.Code
	INNER JOIN [@CVA_VEH_TYPE] TYP
		ON TYP.Code = VEH.U_VehicleType
WHERE CAR.U_CarrierCode = '{0}'</value>
  </data>
  <data name="Invoice_Get" xml:space="preserve">
    <value>SELECT DISTINCT
	'N'				[Selecionar], 
	OINV.DocNum		[Nr. Doc],  
	OINV.Serial		[Nr. NF],
	OINV.CardCode	[Cód. PN],
	OINV.CardName	[Nome PN],
	OINV.DocDate	[Data],
	INV12.Carrier	[Transp.],
	INV12.GrsWeight	[Peso Bruto],
	OCRD.CardName	[Nome Transp.],
	OINV.DocTotal	[Valor Total],
	INV12.[State]	[UF],
	INV12.CityS		[Cidade]
FROM OINV WITH(NOLOCK)
	INNER JOIN [@SKL25NFE] SKILL WITH(NOLOCK)
		ON SKILL.U_DocEntry			= OINV.DocEntry
		AND SKILL.U_tipoDocumento	= 'NS'
		AND SKILL.U_cdErro			= 100
	INNER JOIN INV12 WITH(NOLOCK)
		ON INV12.DocEntry = OINV.DocEntry
	LEFT JOIN OCRD WITH(NOLOCK)
		ON OCRD.CardCode = INV12.Carrier
	
WHERE NOT EXISTS
(
	SELECT TOP 1 1 
	FROM [@CVA_WAYBILL_INV] INV WITH(NOLOCK)
		INNER JOIN [@CVA_WAYBILL] WAYBILL WITH(NOLOCK)
			ON WAYBILL.Code = INV.Code
			AND WAYBILL.U_Status = 1
	WHERE INV.U_DocNum = OINV.DocNum
)
AND OINV.CANCELED = 'N'</value>
  </data>
  <data name="State_Get" xml:space="preserve">
    <value>SELECT * FROM OCST</value>
  </data>
  <data name="VehicleType_Get" xml:space="preserve">
    <value>SELECT * FROM [@CVA_VEH_TYPE]</value>
  </data>
  <data name="Vehicle_Get" xml:space="preserve">
    <value>SELECT 
	VEH.Code,
	TYP.Code		[TypeCode],
	TYP.Name		[TypeDesc],
	VEH.U_Plate		[Plate],
	VEH.U_Color		[Color],
	VEH.U_Driver	[Driver]
FROM [@CVA_VEHICLE] VEH
	INNER JOIN [@CVA_VEH_TYPE] TYP
		ON TYP.Code = VEH.U_VehicleType
WHERE VEH.Code = '{0}'</value>
  </data>
  <data name="Vehicle_GetByCarrierCode" xml:space="preserve">
    <value>SELECT 
	VEH.Code		[Veículo],
	TYP.Name		[Tipo],
	VEH.U_Plate		[Placa],
	VEH.U_Color		[Cor],
	VEH.U_Driver	[Motorista]
FROM [@CVA_VEHICLE] VEH
	INNER JOIN [@CVA_VEH_TYPE] TYP
		ON TYP.Code = VEH.U_VehicleType
	INNER JOIN [@CVA_CAR_VEHICLE] CAR
		ON CAR.U_CodeVehicle = VEH.Code
WHERE CAR.U_CarrierCode = '{0}'</value>
  </data>
  <data name="Vehicle_GetForCombobox" xml:space="preserve">
    <value>SELECT 
	VEH.Code,
	TYP.Name + ' - '  + 
	VEH.U_Color + ' - ' + 
	VEH.U_Plate + ' - ' + 
	VEH.U_Driver	[Description]
FROM [@CVA_VEHICLE] VEH
	INNER JOIN [@CVA_VEH_TYPE] TYP
		ON TYP.Code = VEH.U_VehicleType
	INNER JOIN [@CVA_CAR_VEHICLE] CAR
		ON CAR.U_CodeVehicle = VEH.Code
WHERE CAR.U_CarrierCode = '{0}'</value>
  </data>
