  <data name="Etiqueta_Get" xml:space="preserve">
    <value>SELECT
	FTHAUPT.BELNR_ID Ordem,
	OITM.ItemCode,
	OITM.ItemName,
	FTHAUPT.AUFTRAG		Pedido,
	RESERVIERUNG.ITEM	Serie,
	CASE WHEN ISNULL(RESERVIERUNG.BELNR_ID, 0) = 0
		THEN FTPOS.MENGE_VERBRAUCH
		ELSE 1
	END					Quantidade,
	CASE WHEN OBPL.BPLId =  1
		THEN CASE WHEN ISNULL(ClienteFilial.BPLId, 1) = 1
			THEN OBPL.State
			ELSE ClienteFilial.State
		END
		ELSE OBPL.State
	END UF
FROM BEAS_FTHAUPT FTHAUPT WITH(NOLOCK)
	INNER JOIN BEAS_FTPOS FTPOS WITH(NOLOCK) 
		ON FTPOS.BELNR_ID = FTHAUPT.BELNR_ID
		AND FTPOS.STUFE = 0
	left JOIN OITM WITH(NOLOCK)
		ON OITM.ItemCode = FTPOS.ItemCode
	LEFT JOIN BEAS_FTPOS_RESERVIERUNG RESERVIERUNG WITH(NOLOCK)
		ON RESERVIERUNG.BELNR_ID = FTHAUPT.BELNR_ID 
		AND RESERVIERUNG.BELPOS_ID = FTPOS.BELPOS_ID
	LEFT JOIN ORDR WITH(NOLOCK)
		ON  ORDR.DocNum = BaseDocNum
	LEFT JOIN OBPL WITH(NOLOCK)
		ON OBPL.BPLId = ISNULL(ORDR.BPLId, 1)
	OUTER APPLY
	(
		SELECT TOP 1 OBPL.BPLId, OBPL.State FROM OBPL WITH(NOLOCK)
			INNER JOIN CRD7 WITH(NOLOCK)
				ON CRD7.TaxId0 = OBPL.TaxIdNum
		WHERE CRD7.CardCode = ORDR.CardCode
	)  ClienteFilial</value>
  </data>
  <data name="ItemSerie_Get" xml:space="preserve">
    <value>SELECT 
	OITM.ItemCode,
	OITM.ItemName,
	OSRI.IntrSerial Serie
FROM OITM  WITH(NOLOCK)
	INNER JOIN OSRI WITH(NOLOCK)
		ON OSRI.ItemCode = OITM.ItemCode
WHERE OITM.ItemCode = '{0}'
AND OSRI.IntrSerial = '{1}'</value>
  </data>
  <data name="Item_Get" xml:space="preserve">
    <value>SELECT ItemCode, ItemName FROM OITM WITH(NOLOCK)
WHERE ItemCode = '{0}'</value>
  </data>
  <data name="OP_GetRange" xml:space="preserve">
    <value>SELECT 
	CAST(1 AS BIT)	'Imprimir',
	AUPT.BELNR_ID		'OP',
	AUPT.AUFTRAG		'Pedido',
	AUPT.BELDAT			'Data',
	AUPT.KND_ID			'CardCode',
	AUPT.KNDNAME		'CardName',
	POS.ItemCode		'ItemCode',
	OITM.ItemName		'ItemName',
	POS.MENGE_VERBRAUCH	'Qtde'
 FROM BEAS_FTHAUPT AUPT WITH(NOLOCK)
	INNER JOIN BEAS_FTPOS POS WITH(NOLOCK)
		ON POS.BELNR_ID = AUPT.BELNR_ID
		AND POS.STUFE = 0
	INNER JOIN OITM WITH(NOLOCK)
		ON OITM.ItemCode = POS.ItemCode
		AND ISNULL(OITM.U_TipoProduto, '0') &lt;&gt; '0'
 WHERE 1 = 1</value>
