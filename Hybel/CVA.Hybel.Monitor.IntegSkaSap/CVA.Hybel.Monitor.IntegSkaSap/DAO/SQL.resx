  <data name="Integracao_Get" xml:space="preserve">
    <value>SELECT DISTINCT
	PROD.ID,
	PROD.OP,
	CASE TLOG.[TYPE]
		WHEN 0 THEN 'Registro de Tempo'
		WHEN 1 THEN 'Consumo'
		WHEN 3 THEN 'Produto Acabado'
	END	AS TIPO,
	CASE TLOG.[Status]
		WHEN 0 THEN 'Aguardando Processamento'
		WHEN 1 THEN 'Em Processamento'
		WHEN 2 THEN 'Importado com Sucesso'
		WHEN 3 THEN 'Qtde. Estoque Insuficiente'
		WHEN 4 THEN 'Itens com Posições Inválidas'
		WHEN 5 THEN 'Sem Consumo de Material'
		WHEN 6 THEN 'Gerar Produto Acabado'
		WHEN 7 THEN 'Quantidade Zerada no Consumo'
		WHEN 8 THEN 'Posição OP Fechada'
		WHEN 9 THEN 'Operador não cadastrado no BEAS'
		WHEN 10 THEN 'Posição sem entrada de produto acabado'
		WHEN 99 THEN 'Verificar Observação'
	END [Status],
	TLOG.ID ID_LOG,
	PROD.BELPOS_ID,
	PROD.POS_ID,
	PROD.OPER,
	PROD.QUANT,
	PROD.REJ,
	PROD.OPERADOR,
	PROD.MAQ,
	PROD.CODPECA,
	TLOG.[Description],
	PROD.REPDATETIME
FROM {0}.[dbo].SSPExportProd PROD WITH(NOLOCK)
	INNER JOIN {0}.[dbo].SSPExportProd_Log TLOG WITH(NOLOCK)
		ON TLOG.ID_MES = PROD.ID</value>
  </data>
  <data name="Integracao_UpdateStatus" xml:space="preserve">
    <value>UPDATE {0}.[dbo].SSPExportProd_Log SET [Status] = 0 WHERE ID = {1}</value>
  </data>
  <data name="Integracao_WhereStatus" xml:space="preserve">
    <value>AND '{0}' = 
	CASE WHEN PROD.[STATUS] = 0
		THEN 'Pendente'
		ELSE CASE WHEN ISNULL(TLOG.Description, '') &lt;&gt; '' AND PROD.[STATUS] &lt;&gt; 5
			THEN 'Erro'
			ELSE 'Integrado'
		END
	END</value>
  </data>
