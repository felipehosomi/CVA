  <data name="Branch_Get" xml:space="preserve">
    <value>SELECT 0, 'Todas'
UNION
SELECT BPLId, BPLName + ' - ' + TaxIdNum FROM OBPL
WHERE [Disabled] = 'N'</value>
  </data>
  <data name="Contador_Get" xml:space="preserve">
    <value>SELECT CPF, firstName + ' ' + lastName Nome FROM OHEM
WHERE ContResp = 'Y'</value>
  </data>
  <data name="DimeConfig_Exists" xml:space="preserve">
    <value>SELECT TOP 1 1 FROM [@CVA_DIME]
WHERE U_Filial = {0} AND U_Periodo = '{1}'</value>
  </data>
  <data name="DimeQuadro09_Get" xml:space="preserve">
    <value>SELECT
	'30'			Tipo,
	'09'			Quadro,
	ITEM.U_Item		Item,
	ITEM.U_Valor	Valor
FROM [@CVA_DIME_09] REG
	INNER JOIN [@CVA_DIME_09_ITEM] ITEM
		ON ITEM.Code = REG.Code
WHERE REG.U_Periodo = '{0}'
AND (REG.U_Filial = {1} OR {1} = 0)
AND ISNULL(ITEM.U_Item, '') &lt;&gt; ''
AND (ITEM.U_Valor &gt; 0 OR ITEM.U_Item = '999')</value>
  </data>
  <data name="DimeQuadro12_Get" xml:space="preserve">
    <value>SELECT
	ITEM.U_Origem		Origem,
	ITEM.U_Receita		Receita,
	ITEM.U_Vencimento	Vencimento,
	ITEM.U_Valor		Valor,
	ITEM.U_Classe		Classe,
	ITEM.U_NrAcordo		NrAcordo
FROM [@CVA_DIME_12] REG
	INNER JOIN [@CVA_DIME_12_ITEM] ITEM
		ON ITEM.Code = REG.Code
WHERE REG.U_Periodo = '{0}'
AND (REG.U_Filial = {1} OR {1} = 0)
AND ISNULL(ITEM.U_Origem, 0) &lt;&gt; 0</value>
  </data>
  <data name="DimeQuadro46_Get" xml:space="preserve">
    <value>SELECT
	ITEM.U_Identificacao	Identificacao,
	ITEM.U_Valor		Valor,
	ITEM.U_Origem		Origem
FROM [@CVA_DIME_46] REG
	INNER JOIN [@CVA_DIME_46_ITEM] ITEM
		ON ITEM.Code = REG.Code
WHERE REG.U_Periodo = '{0}'
AND (REG.U_Filial = {1} OR {1} = 0)
AND ISNULL(ITEM.U_Identificacao, '') &lt;&gt; ''</value>
  </data>
  <data name="UserField_GetDescription" xml:space="preserve">
    <value>SELECT 
	T1.FldValue + ' - ' + T1.Descr
FROM CUFD T0 
	INNER JOIN UFD1 T1 
		    ON T0.TableID = T1.TableID AND T0.FieldID = T1.FieldID
WHERE T0.TableID = '{0}'
AND T0.AliasID = '{1}'
AND T1.FldValue = '{2}'</value>
