  <data name="GetCounterToolByItem" xml:space="preserve">
    <value>SELECT 
	T0.U_CVA_Counter AS 'Quantity'
	,T0.U_CVA_Tool AS 'Tool'
FROM OITM T0
WHERE ItemCode = '{0}'</value>
  </data>
  <data name="GetFerramentaItemCode" xml:space="preserve">
    <value>SELECT 
	T0.ResCode AS Id
	,T0.ResName AS Nome
FROM ORSC T0
WHERE T0.ResCode = (SELECT U_CVA_Tool FROM OITM WHERE ItemCode = '{0}')</value>
  </data>
  <data name="GetFerramentas" xml:space="preserve">
    <value>SELECT ResCode AS Id, ResName AS Nome FROM ORSC T0 WHERE U_CVA_ControlaFerramenta = 1</value>
  </data>
  <data name="GetMaquinas" xml:space="preserve">
    <value>select ResCode ,  ResName from orsc where ResType = 'M'</value>
  </data>
  <data name="GetMaxValue" xml:space="preserve">
    <value>select count(code)+1 as 'Code', count(name)+1 as 'Name' from [@CVA_MAQ_PARADAS]</value>
  </data>
  <data name="GetMotivos" xml:space="preserve">
    <value>SELECT code, name FROM [@CVA_MOTIVO]</value>
  </data>
  <data name="GetMquinaParada" xml:space="preserve">
    <value>select *  from [@CVA_MAQ_PARADAS] where U_Maquina = '{0}'</value>
  </data>
  <data name="GetOperador" xml:space="preserve">
    <value>SELECT FIRSTNAME, LASTNAME FROM OHEM</value>
  </data>
  <data name="GetProduto" xml:space="preserve">
    <value>SELECT  oitt.Code
       ,oitm.ItemName 
  FROM oitt
 INNER JOIN OITM
    ON oitt.Code = oitm.itemcode</value>
  </data>
  <data name="GetRecursoValorFixo" xml:space="preserve">
    <value>SELECT ITT1.Code, Quantity FROM ITT1
	INNER JOIN ORSC
		ON ORSC.ResCode			=	ITT1.Code
		AND ORSC.ObjType		=	ITT1.[Type]
		AND ORSC.U_CVA_Fixed	=	1
WHERE Father = '{0}'</value>
  </data>
