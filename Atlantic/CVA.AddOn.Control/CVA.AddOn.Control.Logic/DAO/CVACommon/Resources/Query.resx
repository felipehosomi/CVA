  <data name="Base_GetByName" xml:space="preserve">
    <value>SELECT *,
CASE WHEN ID = (SELECT MIN(ID) FROM CVA_BAS)
	THEN 1
	ELSE 
		CASE WHEN ID = (SELECT MAX(ID) FROM CVA_BAS)
			THEN 2
			ELSE 0
		END
	END TipoBase
FROM CVA_BAS WHERE COMP = '{0}'</value>
    <comment>Retorna base de acordo com nome</comment>
  </data>
  <data name="Base_GetMaxId" xml:space="preserve">
    <value>SELECT MAX(ID) FROM CVA_BAS</value>
    <comment>Retorna o última ID da base</comment>
  </data>
  <data name="Objeto_GetById" xml:space="preserve">
    <value>SELECT * FROM CVA_OBJ
WHERE ID = {0}</value>
    <comment>Retorna o objeto de acordo com o ID</comment>
  </data>
  <data name="Registro_Get" xml:space="preserve">
    <value>SELECT TOP 1
	ID,
	INS		AS 'Insert',
	UPD		AS 'Update',
	STU		AS 'Status',
	BAS		AS 'CodigoBase',
	CODE	AS 'Codigo',
	OBJ		AS 'Objeto',
	FUNC	AS 'Funcao',
	BAS_ERR	AS 'BaseErro'
FROM CVA_REG 
WHERE CODE = '{0}' 
AND OBJ = {1}
ORDER BY INS DESC</value>
    <comment>Retorna o registro</comment>
  </data>
  <data name="Registro_GetErrorList" xml:space="preserve">
    <value>SELECT CODE AS 'Codigo'
FROM CVA_REG
WHERE OBJ = {0}
AND STU = 5
ORDER BY ID</value>
    <comment>Retorna lista dos códigos com erro</comment>
  </data>
  <data name="Registro_UpdateError" xml:space="preserve">
    <value>UPDATE CVA_REG 
SET STU = 3,
UPD = GETDATE()
WHERE OBJ = {0}
AND STU = 5</value>
    <comment>Atualiza registros com erro</comment>
  </data>
  <data name="Timer_Restart" xml:space="preserve">
    <value>UPDATE CVA_TIM 
SET STU = 2,
UPD = GETDATE()</value>
    <comment>Reinicia Timer</comment>
