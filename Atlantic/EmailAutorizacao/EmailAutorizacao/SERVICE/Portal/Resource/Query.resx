  <data name="Config_Get" xml:space="preserve">
    <value>SELECT
	U_Tipo		'Tipo',
	U_Servidor	'Servidor',
	U_Banco		'Banco',
	U_Usuario	'Usuario',
	U_Senha		'Senha'
FROM [@CVA_CONFIG_DB] 
WHERE U_Tipo = {0}</value>
    <comment>Retorna dados de configuração do banco</comment>
  </data>
  <data name="Draft_GetPurchaseOrder" xml:space="preserve">
    <value>SELECT DISTINCT
	ODRF.CreateTS,
	OUSR.U_NAME	UserName,
	OUSR.E_Mail Email,
	ODRF.DocNum,
	ODRF.DocDate,
	ODRF.CardCode,
	ODRF.CardName,
	ODRF.DocTotal,
	OBPL.BPLName
	FROM OWDD
		INNER JOIN WDD1 
			ON WDD1.WddCode = OWDD.WddCode 
			AND WDD1.[Status] = 'W'
		INNER JOIN OUSR
			ON OUSR.USERID = WDD1.UserID
		INNER JOIN ODRF
			ON ODRF.DocEntry = OWDD.DocEntry 
			AND ODRF.ObjType= 22 -- Pedido de Compra
		INNER JOIN OBPL
			ON OBPL.BPLId = ODRF.BPLId
		INNER JOIN OUSR CurrentUser
			ON CurrentUser.USERID = ODRF.UserSign
WHERE OWDD.[Status] = 'W'
AND OWDD.ObjType = 112 -- Esboço
AND ODRF.DocNum = {0}
AND ODRF.DocTotal = {1}
AND ODRF.CreateDate = CAST('{2}' AS DATETIME)
AND SUBSTRING(RIGHT('000000'+ CAST(CreateTS AS NVARCHAR(6)),6), 1, 4) = '{3}'
AND CurrentUser.USER_CODE = '{4}'
ORDER BY
	ODRF.CreateTS</value>
    <comment>Retorna dados do esboço para enviar e-mail ao usuário</comment>
  </data>
  <data name="EmailServer_Get" xml:space="preserve">
    <value>SELECT * FROM CVA_EMAIL_SERVER</value>
    <comment>Retorna dados de configuração do banco</comment>
